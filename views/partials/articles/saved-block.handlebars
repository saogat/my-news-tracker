<div class="panel panel-default">
    <div class="panel-body">
        <p data-id={{this.id}}>
            <a class="article-link" target="_blank" href={{link}}>{{title}}</a>
        </p>
    </div>
    <div class="panel-footer">
        <p>{{summary}}</p>
        <button class="note-button" data-id="{{this.id}}">Notes</button>
    </div>
</div>